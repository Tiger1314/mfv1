public class PushCondition {

    public enum QZ_TYPE{

        NO(1, "不限"),

        ALL(2, "分租"),

        half(3, "整租");

        private int key;
        private String value;

        private QZ_TYPE(int key, String value){
            this.key = key;
            this.value = value;
        }

        /**
         * 通过key 获取value值
         * @param key
         * @return
         */
        public static String getValue(int key) {
            for (QZ_TYPE p : QZ_TYPE.values()) {
                if (p.getKey() == key) {
                    return p.getValue();
                }
            }
            return null;
        }


        public int getKey() {
            return key;
        }

        public void setKey(int key) {
            this.key = key;
        }

        public String getValue() {
            return value;
        }

        public void setValue(String value) {
            this.value = value;
        }
    }

